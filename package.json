{
  "name": "is-iibun",
  "description": "Check if a number is Even with iibun",
  "version": "0.1.5",
  "repository": "ryoppippi/is-iibun",
  "funding": "https://github.com/ryoppippi/is-iibun?sponsor=1",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "latest",
    "@types/is-even": "^1.0.2",
    "bumpp": "^10.1.1",
    "gunshi": "^0.25.0",
    "is-even": "^1.0.0",
    "prime-number": "^2.0.0",
    "publint": "^0.3.12",
    "random-item": "^4.0.1",
    "tsdown": "^0.12.3",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "lint": "biome check .",
    "format": "biome check --write .",
    "build": "tsdown ./*.ts --sourcemap --dts --exports --publint",
    "prepack": "npm run build",
    "release": "bumpp && npm publish"
  },
  "trustedDependencies": [
    "@biomejs/biome"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "*.ts",
    "README.md",
    "docs",
    "LICENSE"
  ],
  "bin": "./dist/cli.mjs",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./dist/cli.mjs",
    "./package.json": "./package.json"
  }
}
